{
	"manifest_version": 2,
	"permissions": ["activeTab", "storage"],

	"name": "earlsh - Chrome plugin",
	"version": "0.1",
	"icons": {
		"128": "images/scissor_icon_128.png"
	},

	"browser_action": {
		"default_title": "Create a shorturl",
		"default_icon": "images/scissor_icon_128.png",
		"default_popup": "popup.html"
	},

	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	}
}